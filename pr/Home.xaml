<Page x:Class="pr.Home"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:pr"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Home">

    <Grid>
        <Button x:Name="button_sphere" Content="Сфера" HorizontalAlignment="Center" Margin="0,46,0,0" VerticalAlignment="Top" Height="66" Width="123" Click="button_sphere_Click"/>
        <Button x:Name="button_cone" Content="Конус" HorizontalAlignment="Center" Margin="0,133,0,0" VerticalAlignment="Top" Height="66" Width="124" Click="button_cone_Click"/>
        <Button x:Name="button_cylinder" Content="Цилиндр" HorizontalAlignment="Center" Margin="0,217,0,0" VerticalAlignment="Top" Height="66" Width="123" Click="button_cylinder_Click"/>
        <Button x:Name="button_truncated_cone" Content="Усечённый конус" HorizontalAlignment="Center" Margin="0,301,0,0" VerticalAlignment="Top" Height="66" Width="123" Click="button_truncated_cone_Click"/>

    </Grid>
</Page>
